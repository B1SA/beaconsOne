PROCEDURE "BEACONSONE"."b1sa.beaconsOne.procedures::getBeaconItemCode" (
                                                            in  BeaconId      NVARCHAR(20),
                                                            out ItemCode    NVARCHAR(50))
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   READS SQL DATA AS
BEGIN
   SELECT   "ItemCode" into ItemCode
   FROM     "_SYS_BIC"."b1sa.beaconsOne.models/SAP_CA_BEACONITEMGROUP"
   WHERE    "BeaconId" = :BeaconId;
END